<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
						http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.3.xsd">
	
	<mvc:resources location="/frontend/build/js/resolved/" mapping="/js/**" />	
	<mvc:resources location="/frontend/build/css/" mapping="/css/**" />
	<mvc:resources location="/frontend/build/vendor/" mapping="/vendor/**" />					
	<mvc:resources location="/frontend/html/main.html" mapping="/main.html" />

</beans>
